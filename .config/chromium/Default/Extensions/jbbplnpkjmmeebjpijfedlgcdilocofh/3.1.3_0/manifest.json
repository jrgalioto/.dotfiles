{
   "background": {
      "all_frames": true,
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "/img/wave16bk.png",
         "32": "/img/wave32bk.png",
         "64": "/img/wave64bk.png"
      },
      "default_title": "WAVE"
   },
   "commands": {
      "toggle-extension": {
         "description": "Send a 'toggle-extension' event to the extension",
         "suggested_key": {
            "default": "Ctrl+Shift+U"
         }
      }
   },
   "content_scripts": [  ],
   "description": "Evaluate web accessibility within the Chrome browser.",
   "icons": {
      "128": "/img/wave128.png",
      "16": "/img/wave16.png",
      "32": "/img/wave32.png",
      "48": "/img/wave48.png",
      "64": "/img/wave64.png",
      "96": "/img/wave96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "WAVE Evaluation Tool",
   "permissions": [ "http://*/*", "https://*/*", "file:///*", "\u003Call_urls>", "tabs", "contextMenus" ],
   "short_name": "WAVE",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.1.3",
   "web_accessible_resources": [ "img/*", "styles/*", "chrome/*", "core/*", "wave/*", "background.js", "bottombar.html", "inject.js", "sidebar.html", "bottombar.html", "topbar.html", "wave.min.js", "sidebar.min.js" ]
